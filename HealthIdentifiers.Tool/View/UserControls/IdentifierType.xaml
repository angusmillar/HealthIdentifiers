<UserControl x:Class="HealthIdentifiers.Tool.View.UserControls.IdentifierType"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:HealthIdentifiers.Tool.View.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="100" d:DesignWidth="450">
    <GroupBox x:Name="LayoutRoot" Margin="10,0,10,0" FontSize="16" FontWeight="Light"
              Header="Identifier Type">
        <StackPanel Margin="5" Orientation="Horizontal" HorizontalAlignment="Center">
            <Image x:Name="ImageIcon"  Source="{Binding Path=IconFilePath}" Margin="0,0,10,0"></Image>
            <Label VerticalAlignment="Center" Content="{Binding Path=FullDisplayName}"  Margin="0,0,0,0"></Label>
            
        </StackPanel>
    </GroupBox>
</UserControl>